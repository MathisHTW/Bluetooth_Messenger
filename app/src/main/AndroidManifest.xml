<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="main">

<application
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/ic_launcher_round"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity android:name="main.Start">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>
    <activity android:name="main.view.CreateChannel" />
    <activity android:name="main.view.CurrentChannel" />
    <activity android:name="main.view.Channel" />
    <activity android:name="main.controller.asap.ASAPActivity" />
    <activity android:name="main.controller.asap.ASAPInit" />
</application>

</manifest>

